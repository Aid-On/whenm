{
  "name": "@aid-on/whenm",
  "version": "0.3.1",
  "description": "Time-aware memory system for LLMs with Event Calculus and Prolog reasoning",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "prolog"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --exclude 'benchmarks/**' --exclude 'tests/_old/**'",
    "test:watch": "vitest --exclude 'benchmarks/**' --exclude 'tests/_old/**'",
    "test:coverage": "vitest run --coverage --exclude 'benchmarks/**' --exclude 'tests/_old/**'",
    "benchmark": "vitest run benchmarks/**/*.test.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@aid-on/unillm": "^0.3.0",
    "@types/kuromoji": "^0.1.3",
    "compromise": "^14.14.5",
    "kuromoji": "^0.1.2",
    "trealla": "^0.28.8",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.9",
    "dotenv": "^16.4.7",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "event-calculus",
    "prolog",
    "temporal-reasoning",
    "llm",
    "memory",
    "edge-computing",
    "cloudflare-workers"
  ],
  "author": "Aid-On Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aid-On/whenm.git"
  },
  "bugs": {
    "url": "https://github.com/Aid-On/whenm/issues"
  },
  "homepage": "https://github.com/Aid-On/whenm#readme",
  "license": "MIT"
}
